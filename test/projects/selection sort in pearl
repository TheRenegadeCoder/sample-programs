@array = (1,45,10,35,100,13,147,500,80);
$size = @array;
$min = 0;

for ($i = 0; $i < $size ; $i=$i+1){

for($j = $i+1  ; $j < $size ; $j=$j+1){
    
    if($array[$j] < $array[$i]){
        
        $min = $array[$j];
        $array[$j] = $array[$i];
        $array[$i] = $min;
        
    }
}

break;

}

foreach $number (@array){

print $number. "
";

}
