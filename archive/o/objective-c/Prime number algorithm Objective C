#import <Foundation/Foundation.h>

int main(int argc, const char * argv[]) {
    @autoreleasepool
    {
        int p, d;
        BOOL isPrime;

        for (p = 2; p <= 50; p++)
        {
            isPrime = true;

            for (d = 2; d < p; d++)
            {
                if (p % d == 0)
                isPrime = false;
            }

            if (isPrime == true)
                NSLog(@"%i", p);
        }
    }
    return 0;
}
